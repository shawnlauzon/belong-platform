{
  "name": "@belongnetwork/types",
  "version": "0.1.5",
  "private": true,
  "type": "module",
  "moduleResolution": "node16",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./database": {
      "types": "./dist/database.d.ts",
      "import": "./dist/database.es.js",
      "require": "./dist/database.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build --config vite.config.mts",
    "dev": "vite build --watch --config vite.config.mts",
    "test": "vitest run --config vitest.config.mts --passWithNoTests",
    "test:watch": "vitest --config vitest.config.mts",
    "test:coverage": "vitest run --coverage --config vitest.config.mts",
    "test:typecheck": "tsc --noEmit -p tsconfig.test.json",
    "typecheck": "tsc --noEmit",
    "gen:db-types": "dotenv -e ../../.env supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/database.ts"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@vitejs/plugin-react": "^4.5.1",
    "dotenv-cli": "^8.0.0",
    "jsdom": "^24.1.3",
    "supabase": "^2.24.3",
    "typescript": "^5.8.3",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^3.8.0",
    "vitest": "^1.6.1"
  }
}
