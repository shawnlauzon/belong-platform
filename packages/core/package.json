{
  "name": "@belongnetwork/core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./transformers": {
      "types": "./dist/transformers/index.d.ts",
      "import": "./dist/transformers/index.js",
      "require": "./dist/transformers/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build --config vite.config.mts",
    "dev": "vite build --watch --config vite.config.mts",
    "test": "vitest run --config vitest.config.mts --passWithNoTests",
    "test:watch": "vitest --config vitest.config.mts",
    "test:coverage": "vitest run --coverage --config vitest.config.mts",
    "test:typecheck": "tsc --noEmit -p tsconfig.test.json",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@belongnetwork/types": "workspace:*",
    "@supabase/supabase-js": "^2.39.7",
    "loglevel": "^1.9.2",
    "nanoid": "^5.1.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.8.0"
  }
}
