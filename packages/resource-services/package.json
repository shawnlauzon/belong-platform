    "@types/jest": "^29.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^24.0.0",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.2.2"
