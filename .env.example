## These are required
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_MAPBOX_PUBLIC_TOKEN=your_mapbox_token

# Set this after you create a user in supabase
SEED_MEMBER_ID=your_user_id

## These are optional
VITE_DEFAULT_LOCATION_LAT=30.2672
VITE_DEFAULT_LOCATION_LNG=-97.7431

# Logging configuration (optional)
# Available levels: trace, debug, info, warn, error, silent
VITE_LOG_LEVEL=debug

SUPABASE_PROJECT_ID=your-project-id

# Push Notifications - Web Push VAPID Keys (optional)
# Generate with: npx web-push generate-vapid-keys
# Required for Edge Function: send-push-notification
VAPID_PUBLIC_KEY=your_vapid_public_key
VAPID_PRIVATE_KEY=your_vapid_private_key
VAPID_SUBJECT=mailto:your-email@example.com

# Email Notifications - Postmark (optional)
# Required for Edge Function: send-email-notification
POSTMARK_NOTIFICATION_SERVER_TOKEN=your_postmark_server_api_token
POSTMARK_NOTIFICATION_FROM_EMAIL=notifications@juntos.community
POSTMARK_NOTIFICATION_TEMPLATE_ID=your_app_notification_template_id
POSTMARK_NOTIFICATION_MESSAGE_STREAM=notification-transaction-stream

# Required for deep links in emails
VITE_APP_URL=https://juntos.community