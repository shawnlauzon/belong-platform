{
  "name": "belongnetwork",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "packages/api",
    "packages/types",
    "packages/core"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r --parallel --no-bail test",
    "test:coverage": "pnpm -r --parallel --no-bail test:coverage",
    "test:watch": "pnpm -r --parallel test -- --watch",
    "typecheck": "pnpm -r typecheck",
    "typecheck:api": "pnpm --filter=@belongnetwork/api typecheck",
    "lint": "pnpm -r lint",
    "format": "prettier --write .",
    "manypkg": "manypkg check",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@manypkg/cli": "^0.24.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^3.8.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.6.1"
  },
  "packageManager": "pnpm@8.10.0"
}
