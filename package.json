{
  "name": "belong-network",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter @belongnetwork/app dev",
    "build": "pnpm db:types && pnpm -r build",
